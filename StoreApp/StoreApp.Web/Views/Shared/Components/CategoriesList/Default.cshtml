@model IEnumerable<CategoryViewModel>

<div class="d-grid gap-1">
    <a class="btn btn-outline-secondary btn-block" asp-action="Index" 
    asp-controller="Home">Anasayfa</a>

    @foreach (var category in Model)
    {
        <a class="btn btn-outline-secondary btn-block" asp-action="Index" 
        asp-controller="Home">@category.Name</a>
    }
</div>